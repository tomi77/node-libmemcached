  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/node-libmemcached/issues"
  },
  "homepage": "https://github.com/tomi77/node-libmemcached",
  "dependencies": {
    "nan": "^2.8.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^4.0.1"
  }
}
